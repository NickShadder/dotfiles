# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[Documents/**]
end_of_line = crlf

[private_dot_config/**.lua]
indent_style = space
indent_size = 2
